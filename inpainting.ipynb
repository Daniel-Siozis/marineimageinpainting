    "import numpy as np\n",
    "import cv2\n",
    "img = cv2.imread('img/zelda.jpg')\n",
    "mask = cv2.imread('img/zeldamask.png',0)\n",
    "dst = cv2.inpaint(img,mask,3,cv2.INPAINT_TELEA)\n",
    "cv2.imwrite('zelda1_scratch_inpainted.jpg', dst)\n",
    "cv2.imshow('dst',dst)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
